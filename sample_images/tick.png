	autocomplete_adresse = new google.maps.places.Autocomplete(input_adresse, options_adresse);
	
	autocomplete_adresse.addListener('place_changed', function() {
		var infos_ville = autocomplete_adresse.getPlace();
		var componentForm = {
			street_number: 'short_name',
			route: 'long_name',
			locality: 'long_name',
			administrative_area_level_1: 'short_name',
			country: 'long_name',
			postal_code: 'short_name'
		};

		for (var i = 0; i < infos_ville.address_components.length; i++) 
		{
			var addressType = infos_ville.address_components[i].types[0];
			if(componentForm[addressType]) 
			{
				var val = infos_ville.address_components[i][componentForm[addressType]];
				if(addressType == 'street_number')
				{
					$('#esti_mini #no').val(val);
				}

				if(addressType == 'route') 
				{
					$('#esti_mini #rue').val(val);
				}

				if(addressType == 'locality') 
				{
					var valCity = val;
				}

				if(addressType == 'postal_code') 
				{
					var valCp = val;
				}
			}
		}
		$('#esti_mini #ville').val(valCp + " " + valCity);
		console.log(valCity);
		console.log(valCp);
	});
	
	$('#esti_mini #Adresse').on('keydown', function(event) {
		var x = event.which;
		if (x === 13) {
			event.preventDefault();
		}
	});
}
